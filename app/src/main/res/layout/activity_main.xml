<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="codepig.webviewtest.MainActivity">
<LinearLayout
    android:orientation="vertical"
    android:layout_width="368dp"
    android:layout_height="wrap_content">
    <TextView
        android:text="测试在线页面"
        android:textColor="@android:color/white"
        android:background="@android:color/darker_gray"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />
    <TextView
        android:text="userAgent:"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />
    <EditText
        android:id="@+id/userAgent_t"
        android:text="Mozilla/5.0 (Linux; Android 5.0.2; MI 2 Build/LRX22G; wv) AppleWebKit/537.36 (KHTML, like Gecko) Version/4.0 Chrome/55.0.2883.91 Mobile Safari/537.36 boosj_2.7.0"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />
    <TextView
        android:text="url:"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />
    <EditText
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/url_t"
        android:text="http://www.boosj.com/subject_90547.html"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent" />
    <Button
        android:text="go"
        android:id="@+id/goBtn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />
    <!--<View-->
        <!--android:layout_marginBottom="10dp"-->
        <!--android:layout_marginTop="10dp"-->
        <!--android:background="@android:color/darker_gray"-->
        <!--android:layout_width="match_parent"-->
        <!--android:layout_height="1dp" />-->
    <TextView
        android:text="测试main/assets目录下的本地test.html"
        android:layout_marginTop="10dp"
        android:textColor="@android:color/white"
        android:background="@android:color/darker_gray"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />
    <Button
        android:text="js test"
        android:id="@+id/jsTestBtn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />
</LinearLayout>
</android.support.constraint.ConstraintLayout>
